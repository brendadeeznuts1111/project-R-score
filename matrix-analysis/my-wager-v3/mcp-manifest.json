{
  "name": "bun-test-configuration",
  "version": "1.3.8-tier-1380",
  "description": "Tier-1380 Secure Test Configuration with Quantum-Resistant Seals",
  "schemes": {
    "bun": {
      "handlers": {
        "test/config": {
          "description": "Tier-1380 Test Configuration Endpoint",
          "methods": ["GET", "POST"],
          "security": {
            "type": "zero-trust",
            "scheme": "quantum-seal"
          },
          "parameters": {
            "region": {
              "type": "string",
              "description": "AWS region for configuration",
              "enum": ["us-east-1", "us-west-2", "eu-west-1", "ap-southeast-1", "sa-east-1"],
              "default": "us-east-1"
            },
            "environment": {
              "type": "string",
              "description": "Test environment",
              "enum": ["ci", "staging", "production"],
              "default": "ci"
            },
            "sealId": {
              "type": "string",
              "description": "Quantum seal ID for verification",
              "pattern": "^tier1380-[0-9]+-[a-z0-9]+$"
            }
          },
          "responses": {
            "200": {
              "description": "Configuration retrieved successfully",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "config": {
                        "type": "object",
                        "description": "Parsed TOML configuration"
                      },
                      "seal": {
                        "type": "string",
                        "description": "Quantum-resistant seal"
                      },
                      "parseTime": {
                        "type": "number",
                        "description": "Parse time in milliseconds"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "test/coverage": {
          "description": "Test Coverage with Threshold Enforcement",
          "methods": ["GET", "POST"],
          "parameters": {
            "threshold": {
              "type": "number",
              "minimum": 0,
              "maximum": 1,
              "description": "Coverage threshold (0.0 - 1.0)",
              "default": 0.9
            },
            "format": {
              "type": "string",
              "enum": ["text", "lcov", "html", "json"],
              "default": "json"
            },
            "region": {
              "type": "string",
              "description": "Region for coverage report",
              "enum": ["us-east-1", "us-west-2", "eu-west-1", "ap-southeast-1", "sa-east-1"]
            }
          },
          "responses": {
            "200": {
              "description": "Coverage report generated",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "summary": {
                        "type": "object",
                        "properties": {
                          "lines": { "type": "number" },
                          "functions": { "type": "number" },
                          "branches": { "type": "number" },
                          "statements": { "type": "number" }
                        }
                      },
                      "meetsThreshold": { "type": "boolean" },
                      "seal": { "type": "string" }
                    }
                  }
                }
              }
            }
          }
        },
        "test/execute": {
          "description": "Execute Tier-1380 Test Suite",
          "methods": ["POST"],
          "security": {
            "type": "zero-trust",
            "scheme": "quantum-seal",
            "requirements": ["csrf-token", "quantum-signature"]
          },
          "parameters": {
            "testSuite": {
              "type": "string",
              "description": "Test suite to execute",
              "default": "all"
            },
            "parallel": {
              "type": "boolean",
              "description": "Run tests in parallel",
              "default": true
            },
            "timeout": {
              "type": "number",
              "description": "Test timeout in milliseconds",
              "minimum": 5000,
              "maximum": 300000,
              "default": 30000
            }
          },
          "responses": {
            "200": {
              "description": "Tests executed successfully",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "exitCode": { "type": "number" },
                      "duration": { "type": "number" },
                      "coverage": { "$ref": "#/components/schemas/CoverageReport" },
                      "sealId": { "type": "string" },
                      "artifacts": {
                        "type": "array",
                        "items": { "type": "string" }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "test/verify": {
          "description": "Verify Zero-Trust Test Environment",
          "methods": ["POST"],
          "security": {
            "type": "zero-trust",
            "scheme": "quantum-seal"
          },
          "parameters": {
            "fullScan": {
              "type": "boolean",
              "description": "Perform full security scan",
              "default": false
            }
          },
          "responses": {
            "200": {
              "description": "Verification complete",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": { "type": "string", "enum": ["PASSED", "FAILED"] },
                      "violations": {
                        "type": "array",
                        "items": { "$ref": "#/components/schemas/SecurityViolation" }
                      },
                      "report": { "type": "string" }
                    }
                  }
                }
              }
            }
          }
        },
        "test/rss": {
          "description": "RSS Feed for Test Executions",
          "methods": ["GET", "POST"],
          "parameters": {
            "format": {
              "type": "string",
              "enum": ["xml", "json", "markdown"],
              "default": "xml"
            },
            "limit": {
              "type": "number",
              "minimum": 1,
              "maximum": 100,
              "default": 50
            }
          },
          "responses": {
            "200": {
              "description": "RSS feed retrieved",
              "content": {
                "application/rss+xml": {
                  "schema": {
                    "type": "string",
                    "description": "RSS XML feed"
                  }
                }
              }
            }
          }
        },
        "test/dashboard": {
          "description": "Real-time Performance Dashboard",
          "methods": ["GET"],
          "websocket": true,
          "responses": {
            "200": {
              "description": "Dashboard HTML",
              "content": {
                "text/html": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "resources": [
    {
      "type": "test-configuration",
      "uri": "bun://test/config",
      "name": "Tier-1380 Test Config",
      "description": "Secure test configuration with inheritance and quantum seals",
      "attributes": {
        "tier": 1380,
        "security": "quantum-resistant",
        "compliance": ["col-93", "gb9c", "zero-trust"],
        "regions": ["us-east-1", "us-west-2", "eu-west-1", "ap-southeast-1", "sa-east-1"],
        "parseTimeTarget": "<1ms",
        "features": ["toml-inheritance", "conditional-config", "regional-deployment"]
      }
    },
    {
      "type": "coverage-report",
      "uri": "bun://test/coverage",
      "name": "Test Coverage Report",
      "description": "Quantum-sealed coverage artifacts with threshold enforcement",
      "attributes": {
        "threshold": 0.9,
        "formats": ["text", "lcov", "html", "json"],
        "sealed": true,
        "auditTrail": true
      }
    },
    {
      "type": "test-execution",
      "uri": "bun://test/execute",
      "name": "Test Execution Engine",
      "description": "Secure test execution with zero-trust verification",
      "attributes": {
        "parallel": true,
        "timeout": 30000,
        "artifacts": true,
        "rssBroadcast": true
      }
    },
    {
      "type": "security-verification",
      "uri": "bun://test/verify",
      "name": "Zero-Trust Verifier",
      "description": "Comprehensive security verification for test environment",
      "attributes": {
        "csrfProtection": true,
        "namingConvention": "com.tier1380.*",
        "quantumResistant": true,
        "auditTrail": true
      }
    },
    {
      "type": "rss-feed",
      "uri": "bun://test/rss",
      "name": "Test Execution RSS Feed",
      "description": "Real-time RSS feed with quantum-resistant seals",
      "attributes": {
        "format": "rss-2.0",
        "quantumSeal": true,
        "changelog": true,
        "retention": 100
      }
    },
    {
      "type": "performance-dashboard",
      "uri": "bun://test/dashboard",
      "name": "Regional Performance Dashboard",
      "description": "Real-time monitoring dashboard for all regions",
      "attributes": {
        "realTime": true,
        "websocket": true,
        "regions": 5,
        "metrics": ["parseTime", "passRate", "coverage", "duration"]
      }
    }
  ],
  "components": {
    "schemas": {
      "CoverageReport": {
        "type": "object",
        "properties": {
          "summary": {
            "type": "object",
            "properties": {
              "lines": { "type": "number", "minimum": 0, "maximum": 1 },
              "functions": { "type": "number", "minimum": 0, "maximum": 1 },
              "branches": { "type": "number", "minimum": 0, "maximum": 1 },
              "statements": { "type": "number", "minimum": 0, "maximum": 1 }
            }
          },
          "meetsThreshold": { "type": "boolean" },
          "seal": { "type": "string", "description": "Quantum-resistant seal" }
        }
      },
      "SecurityViolation": {
        "type": "object",
        "properties": {
          "type": { "type": "string" },
          "severity": { "type": "string", "enum": ["LOW", "MEDIUM", "HIGH", "CRITICAL"] },
          "message": { "type": "string" },
          "timestamp": { "type": "number" },
          "remediation": { "type": "string" }
        }
      },
      "QuantumSeal": {
        "type": "object",
        "properties": {
          "sealId": { "type": "string" },
          "algorithm": { "type": "string", "default": "SHA-512" },
          "signature": { "type": "string" },
          "timestamp": { "type": "number" },
          "version": { "type": "string", "default": "1.3.8-tier-1380" }
        }
      }
    },
    "securitySchemes": {
      "quantum-seal": {
        "type": "apiKey",
        "description": "Quantum-resistant seal for authentication",
        "name": "X-Quantum-Seal",
        "in": "header"
      },
      "zero-trust": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "Zero-trust authentication with quantum-resistant signatures"
      }
    }
  },
  "metadata": {
    "tier": 1380,
    "version": "1.3.8",
    "baseSecurity": {
      "level": "quantum-resistant",
      "compliance": ["col-93", "gb9c", "zero-trust"],
      "auditTrail": true
    },
    "performance": {
      "parseTimeTarget": "<1ms",
      "col93Alignment": true,
      "bytecodeMetrics": true,
      "gb9cSupport": true
    },
    "deployment": {
      "regions": 5,
      "configurationInheritance": true,
      "conditionalSections": true
    },
    "monitoring": {
      "rssFeed": true,
      "dashboard": true,
      "realTime": true,
      "websocket": true
    },
    "security": {
      "messageEventSource": {
        "uri": "bun://reference/globals/MessageEvent/source",
        "mimeType": "application/json",
        "name": "MessageEvent.source Security Reference",
        "metadata": {
          "securityClassification": "critical",
          "zeroTrustPattern": "event.source === expectedPort",
          "bunVersion": "1.0.0+",
          "col93Width": 19,
          "lastUpdated": "2026-01-30T17:14:00Z",
          "compliance": ["tier-1380", "zero-trust", "csrf-protection"]
        }
      }
    }
  }
}
