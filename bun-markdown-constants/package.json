{
  "name": "@bun-tools/markdown-constants",
  "version": "1.0.0",
  "description": "Enterprise-grade markdown constants and presets for Bun",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts"
    },
    "./utils": {
      "types": "./src/utils.ts",
      "import": "./src/utils.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "verify": "bun test/verify.ts",
    "benchmark": "bun benchmark/performance.bench.ts",
    "demo": "bun demo/demo.ts"
  },
  "keywords": [
    "bun",
    "markdown",
    "constants",
    "enterprise",
    "typescript"
  ],
  "author": "Bun Tools Team",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0"
  }
