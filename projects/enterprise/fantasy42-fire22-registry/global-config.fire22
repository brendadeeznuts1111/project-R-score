[backup]
# Backup Configuration
enabled = true
schedule = "0 2 * * *"
retention_days = 30
encryption = true

[backup.locations]
# Backup Storage
cloudflare_r2 = { enabled = true, bucket = "fantasy42-backups" }
github_releases = { enabled = true, tag = "backup" }
local_storage = { enabled = true, path = "./backups" }

[recovery]
# Recovery Configuration
auto_recovery = true
disaster_recovery = true
multi_region = false
failover_enabled = true

