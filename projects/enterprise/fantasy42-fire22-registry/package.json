{
  "name": "fantasy42-fire22-registry",
  "version": "5.1.0",
  "description": "Enterprise-grade Fantasy42-Fire22 package registry",
  "author": {
    "name": "Fire22 Development Team",
    "email": "enterprise@fire22.com",
    "url": "https://fire22.com"
  },
  "license": "MIT",
  "homepage": "https://fire22.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/fantasy42-fire22/registry.git"
  },
  "bugs": {
    "url": "https://github.com/fantasy42-fire22/registry/issues",
    "email": "support@fire22.com"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/fire22"
  },
  "keywords": [
    "fantasy42",
    "fire22",
    "package-registry",
    "enterprise",
    "bun",
    "typescript",
    "fantasy-sports",
    "gaming",
    "registry",
    "npm",
    "monorepo"
  ],
  "main": "src/index.ts",
  "engines": {
    "bun": ">=1.3.8",
    "node": ">=18.0.0"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "license-checker": "^25.0.1",
    "lint-staged": "^16.2.7",
    "snyk": "^1.1302.1",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.1"
  },
  "peerDependencies": {
    "@types/react": "^19.1.12"
  },
  "optionalDependencies": {},
  "scripts": {
    "dev": "bun run --hot --console-depth=6 src/index.ts",
    "start": "NODE_ENV=production bun run --sql-preconnect src/index.ts",
    "api-server": "bun run api-server-bun-fixed.ts",
    "api-server:dev": "bun run --hot api-server-bun-fixed.ts",
    "start:fast": "NODE_ENV=production bun run --sql-preconnect --console-depth=2 src/index.ts",
    "test": "bun test",
    "test:unit": "bun test testing/unit",
    "test:integration": "bun test testing/integration",
    "test:e2e": "bun test testing/e2e",
    "test:performance": "bun test testing/performance",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch --console-depth=4",
    "test:all": "bun run test:unit && bun run test:integration && bun run test:e2e",
    "test:comprehensive": "bun run scripts/comprehensive-edge-case-tester.ts --full-suite && bun run scripts/playbook-auditor.ts --all-repos --output markdown",
    "test:ci": "bun run test:comprehensive && bun run test:coverage",
    "test:debug": "bun test --console-depth=8",
    "test:report": "bun run scripts/test-report.ts",
    "build:production": "bun run build-production.ts all",
    "build:bundle": "bun run build-production.ts build",
    "build:docker": "bun run build-production.ts docker",
    "build:nginx": "bun run build-production.ts nginx",
    "build:deploy": "bun run build-production.ts deploy",
    "production": "NODE_ENV=production bun run src/production-server.ts",
    "production:ultra": "NODE_ENV=production bun run src/ultra-fast-registry.ts",
    "production:standalone": "bun run src/standalone-executable.ts",
    "build:standalone": "bun run src/standalone-executable.ts compile",
    "demo:performance": "bun run src/bun-performance-demo.ts",
    "demo:optimized": "bun run src/bun-optimized-server.ts",
    "deploy:production": "./deploy-production.sh",
    "health:check": "./health-check.sh",
    "db:init": "bun run --sql-preconnect scripts/db-init.ts",
    "db:seed": "bun run --sql-preconnect scripts/seed.ts",
    "db:migrate": "bun run --sql-preconnect scripts/migrate.ts",
    "db:backup": "bun run --sql-preconnect scripts/backup.ts",
    "db:health": "bun run --console-depth=3 scripts/health-check.ts",
    "db:fast": "bun run --sql-preconnect --console-depth=1 scripts/db-init.ts",
    "security:setup": "bun run scripts/setup-security-env.ts",
    "security:audit": "bun run scripts/security-audit.ts",
    "security:install-tools": "bun add -d snyk license-checker",
    "security:vulnerabilities": "snyk test --json > security-reports/snyk-report.json || echo 'Snyk scan completed with issues'",
    "security:licenses": "license-checker --json --out security-reports/licenses.json",
    "security:bun-audit": "bun audit --audit-level moderate",
    "security:full-scan": "bun run security:audit",
    "security:ci": "bun run security:bun-audit && bun run security:vulnerabilities",
    "security:pre-commit": "bun run security:bun-audit",
    "stylelint": "stylelint '**/*.css' --config .stylelintrc",
    "stylelint:fix": "stylelint '**/*.css' --config .stylelintrc --fix",
    "stylelint:check": "stylelint '**/*.css' --config .stylelintrc --max-warnings 0",
    "lint:fix": "bunx --package eslint eslint --fix --filter='@fire22/*'",
    "lint:fix:ts": "bunx --package @typescript-eslint/eslint-plugin eslint --fix --filter='@fire22/*'",
    "upgrade:dry-run": "bash scripts/upgrade-dry.sh",
    "upgrade:dry-run:ws": "bash scripts/upgrade-dry.sh",
    "build:all": "bash scripts/monobuild.sh",
    "changeset:create": "bash scripts/bunx-wrapper.sh @changesets/cli changeset",
    "changeset:version": "bash scripts/bunx-wrapper.sh @changesets/cli version",
    "changeset:publish": "bash scripts/bunx-wrapper.sh @changesets/cli publish",
    "registry:fix": "bun run scripts/fix-registry-connection.ts fix",
    "registry:status": "bun run scripts/fix-registry-connection.ts status",
    "version:audit": "bun run scripts/version-sync.ts report",
    "version:sync": "bun run scripts/version-sync.ts sync",
    "version:fix": "bun run scripts/version-sync.ts fix",
    "meta:audit": "bun run scripts/meta-sync.ts report",
    "meta:sync": "bun run scripts/meta-sync.ts sync",
    "build:windows": "bun run scripts/build-windows.ts",
    "build:windows:dev": "bun run scripts/build-windows.ts --mode=development",
    "build:windows:prod": "bun run scripts/build-windows.ts --mode=production",
    "build:windows:enterprise": "bun run scripts/build-windows.ts --mode=enterprise",
    "build:windows:sign": "bun run scripts/build-windows.ts --mode=production --sign",
    "build:windows:all": "bun run scripts/build-windows.ts --all-targets",
    "build:cross-platform": "bun run scripts/build-cross-platform.ts",
    "build:all-packages:windows": "bun run scripts/build-all-packages-windows.ts",
    "build:benchmark:windows": "bun run enterprise/packages/scripts/scripts/build-benchmark-orchestrator-windows.ts",
    "sign:windows": "bun run scripts/sign-windows-executable.ts",
    "package:windows": "bun run scripts/package-windows-installer.ts",
    "test:windows": "bun run scripts/test-windows-builds.ts",
    "test:windows:verbose": "bun run scripts/test-windows-builds.ts --verbose",
    "test:fantasy402": "bun run scripts/test-fantasy402-integration.ts",
    "test:fantasy402:setup": "cp .env.fantasy402.example .env.fantasy402 && echo 'Please edit .env.fantasy402 with your credentials'",
    "test:edge-cases": "bun run scripts/comprehensive-edge-case-tester.ts --full-suite",
    "audit": "bun audit",
    "audit:ci": "bun audit --format json",
    "security:scan": "bunx --package @fire22-registry/security-scanner scan .",
    "test:performance:fantasy402": "bun test testing/performance/fantasy402-performance.test.ts",
    "test:workbench": "bun run testing/workbench/workbench-server.ts",
    "test:workbench:start": "bun run testing/workbench/workbench-server.ts &",
    "test:workbench:stop": "pkill -f workbench-server.ts",
    "playbook:audit": "bun run scripts/playbook-auditor.ts --all-repos --output markdown",
    "playbook:audit:json": "bun run scripts/playbook-auditor.ts --all-repos --output json",
    "arb:validate": "bun run scripts/arb-design-doc-validator.ts",
    "compliance:check": "bun run scripts/playbook-auditor.ts --repo . --output console",
    "windows:complete": "bun run build:all-packages:windows && bun run sign:windows dist/**/*.exe && bun run package:windows",
    "windows:validate": "bun run build:all-packages:windows && bun run test:windows",
    "fantasy402:init": "bun run test:fantasy402:setup && bun run test:fantasy402",
    "fantasy402:workbench": "bun run --sql-preconnect --console-depth=5 scripts/start-fantasy402-workbench.ts",
    "fantasy402:workbench:dev": "bun run --sql-preconnect --console-depth=8 scripts/start-fantasy402-workbench.ts --log-level debug",
    "fantasy402:workbench:prod": "bun run --sql-preconnect --console-depth=2 scripts/start-fantasy402-workbench.ts --no-open --log-level warn",
    "fantasy402:workbench:fast": "bun run --sql-preconnect --console-depth=1 scripts/start-fantasy402-workbench.ts --no-open --log-level error",
    "perf:startup": "time bun run --sql-preconnect src/index.ts",
    "perf:memory": "bun run --sql-preconnect scripts/memory-profile.ts",
    "perf:benchmark": "bun run --console-depth=3 scripts/benchmark.ts",
    "perf:fileops": "bun run --console-depth=4 scripts/enhanced-file-operations.bun.ts performance",
    "analyze:files": "bun run scripts/enhanced-file-operations.bun.ts files",
    "analyze:sourcemaps": "bun run scripts/enhanced-file-operations.bun.ts sourcemaps",
    "analyze:network": "bun run scripts/enhanced-file-operations.bun.ts network",
    "demo:new-features": "bun run scripts/enhanced-file-operations.bun.ts all",
    "demo:bun-showcase": "bun run --console-depth=6 scripts/bun-performance-showcase.bun.ts",
    "registry:build": "bun run build-registry.bun.ts",
    "registry:publish": "bun run scripts/publish-registry.bun.ts",
    "registry:validate": "bun run scripts/publish-registry.bun.ts --validate-only",
    "registry:semver-check": "bun run scripts/publish-registry.bun.ts --semver-check",
    "registry:department-validation": "bun run scripts/publish-registry.bun.ts --department-validation",
    "department:validate": "bun run scripts/department-validation.bun.ts",
    "department:security": "bun run scripts/department-validation.bun.ts security-compliance",
    "department:technology": "bun run scripts/department-validation.bun.ts technology",
    "department:design": "bun run scripts/department-validation.bun.ts design",
    "department:product": "bun run scripts/department-validation.bun.ts product-management",
    "department:operations": "bun run scripts/department-validation.bun.ts operations",
    "department:finance": "bun run scripts/department-validation.bun.ts finance",
    "department:management": "bun run scripts/department-validation.bun.ts management",
    "department:marketing": "bun run scripts/department-validation.bun.ts marketing",
    "department:contributors": "bun run scripts/department-validation.bun.ts team-contributors",
    "department:onboarding": "bun run scripts/department-validation.bun.ts onboarding",
    "notify:department-heads": "bun run scripts/department-heads-notification.bun.ts",
    "notify:send": "bun run scripts/department-heads-notification.bun.ts send",
    "notify:reminders": "bun run scripts/department-heads-notification.bun.ts reminders",
    "notify:process": "bun run scripts/department-heads-notification.bun.ts process",
    "notify:rss": "bun run scripts/department-heads-notification.bun.ts rss",
    "notify:blog": "bun run scripts/department-heads-notification.bun.ts blog",
    "notify:status": "bun run scripts/department-heads-notification.bun.ts status",
    "branding:audit": "bunx @fire22/branding-audit audit",
    "branding:audit:report": "bunx @fire22/branding-audit report html",
    "branding:audit:demo": "bun run packages/branding-audit/demo.ts",
    "format": "bunx prettier@3.2.5 --write .",
    "format:check": "bunx prettier@3.2.5 --check .",
    "format:fix": "bunx prettier@3.2.5 --write .",
    "lint": "bunx eslint --ext .js,.ts,.tsx,.jsx src/ enterprise/ dashboard-worker/ scripts/ testing/",
    "ts-node": "bunx ts-node",
    "global:setup": "bun run scripts/global-sportsbet-manager.ts setup",
    "global:install": "bun run scripts/global-sportsbet-manager.ts install",
    "global:sync": "bun run scripts/global-sportsbet-manager.ts sync",
    "global:test": "bun run scripts/global-sportsbet-manager.ts test",
    "global:audit": "bun run scripts/global-sportsbet-manager.ts audit",
    "global:examples": "bun run scripts/global-sportsbet-manager.ts examples",
    "global:full": "bun run scripts/global-sportsbet-manager.ts",
    "global:list": "bun pm ls --global",
    "global:update": "bun update --global",
    "global:clean": "bun pm prune --global && rm -rf ~/.bun/install/cache/*",
    "global:verify": "bun run scripts/verify-global-integration.ts",
    "bunx:cache:clear": "rm -rf ~/.bun/bunx/cache/*",
    "bunx:cache:size": "du -sh ~/.bun/bunx/cache",
    "bunx:test:sportsbet": "bunx @sportsbet-registry/cli --version",
    "bunx:test:fire22": "bunx @fire22/security-scanner --version",
    "bunx:test:scope": "bunx security-scanner --version",
    "global:enhance": "bun run scripts/enhance-global-config.ts",
    "global:cache:status": "ls -la ~/.bun/install/cache/ && ls -la ~/.bun/bunx/cache/",
    "global:registry:test": "curl -s -o /dev/null -w '%{http_code}' https://registry.sportsbet.com/",
    "global:aliases:load": "source ~/.bun/aliases.sh && echo 'Aliases loaded!'",
    "global:perf:test": "time bunx prettier --version && time bun pm ls --global",
    "sb": "./scripts/quick-commands.sh",
    "sb:cache": "./scripts/quick-commands.sh cache",
    "sb:list": "./scripts/quick-commands.sh list",
    "sb:audit": "./scripts/quick-commands.sh audit",
    "sb:perf": "./scripts/quick-commands.sh perf",
    "sb:clean": "./scripts/quick-commands.sh clean",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,html,css,scss,md,toml,yml,yaml}": [
      "bun run format:fix"
    ],
    "*.{js,ts,tsx,jsx}": [
      "bun run lint"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.factory-wager.com"
  },
  "trustedDependencies": [
    "snyk"
  ]
}
