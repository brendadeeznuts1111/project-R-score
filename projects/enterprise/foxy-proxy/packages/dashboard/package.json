{
  "name": "ipfoxy-dashboard",
  "private": true,
  "version": "1.0.0",
  "description": "IPFoxy Proxy Management Dashboard",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/main.tsx",
    "build": "node scripts/build-with-defines.js",
    "preview": "bun run dist/index.mjs",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "serve": "bun cli.ts serve 3000",
    "serve:3000": "bun cli.ts serve 3000",
    "serve:8080": "bun cli.ts serve 8080",
    "serve:8081": "bun cli.ts serve 8081",
    "benchmark": "bun cli.ts benchmark",
    "cli": "bun cli.ts help",
    "deploy": "bun deploy-to-r2.ts",
    "deploy:r2": "bun run build && bun deploy-to-r2.ts",
    "deploy:dev": "bun run build && bun deploy-to-r2.ts --dev",
    "deploy:staging": "bun run build && bun deploy-to-r2.ts --staging",
    "deploy:prod": "bun run build && bun deploy-to-r2.ts --prod",
    "r2-manage": "bun r2-cli.ts"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.8.0",
    "lucide-react": "^0.263.1",
    "clsx": "^1.2.1",
    "recharts": "^2.5.0",
    "date-fns": "^2.29.3",
    "axios": "^1.6.0",
    "@aws-sdk/client-s3": "^3.965.0",
    "@aws-sdk/s3-request-presigner": "^3.965.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "1.3.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitest/ui": "4.0.16",
    "jsdom": "27.4.0",
    "typescript": "~5.9.3",
    "vitest": "4.0.16"
  },
  "bundledDependencies": [
    "react",
    "react-dom",
    "react-router-dom",
    "lucide-react",
    "clsx",
    "recharts",
    "date-fns",
    "axios",
    "@aws-sdk/client-s3",
    "@aws-sdk/s3-request-presigner"
  ]
}
