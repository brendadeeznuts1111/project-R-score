{
  "name": "@duoplus/core",
  "version": "1.0.0",
  "description": "DuoPlus Core - Enterprise dependency injection and performance monitoring framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./di": {
      "import": "./dist/di/index.js",
      "types": "./dist/di/index.d.ts"
    },
    "./monitoring": {
      "import": "./dist/monitoring/index.js",
      "types": "./dist/monitoring/index.d.ts"
    },
    "./testing": {
      "import": "./dist/testing/index.js",
      "types": "./dist/testing/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "lint": "bunx eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/node": "^25.0.8",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "bun": ">=1.1.40"
  },
  "keywords": [
    "dependency-injection",
    "performance-monitoring",
    "testing",
    "enterprise",
    "bun",
    "typescript"
  ],
  "author": "DuoPlus Enterprise Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/duoplus/enterprise-components.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "registry": "https://registry.duoplus.com",
    "access": "public"
  }
}
