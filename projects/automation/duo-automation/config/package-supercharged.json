/**
 * Enhanced package.json with Supercharged CLI Dependencies
 * 
 * Enterprise-grade dependencies for JSONPath, JQ-like filtering,
 * interactive TUI, and advanced analytics capabilities.
 */

{
  "name": "@duoplus/supercharged-inspection",
  "version": "2.0.0",
  "description": "Supercharged DuoPlus CLI Inspection System with JSONPath, JQ-like filtering, interactive TUI, and enterprise analytics",
  "main": "src/@inspection/supercharged-cli.ts",
  "type": "module",
  "scripts": {
    "inspect": "bun run src/@inspection/supercharged-cli.ts",
    "inspect:interactive": "bun run src/@inspection/supercharged-cli.ts --interactive",
    "inspect:analytics": "bun run src/@inspection/supercharged-cli.ts --analytics",
    "inspect:security": "bun run src/@inspection/supercharged-cli.ts --security --patterns",
    "inspect:html": "bun run src/@inspection/supercharged-cli.ts --format=html --output=inspection.html",
    "inspect:tree": "bun run src/@inspection/supercharged-cli.ts --tree",
    "inspect:jsonpath": "bun run src/@inspection/supercharged-cli.ts --jsonpath",
    "inspect:jq": "bun run src/@inspection/supercharged-cli.ts --jq",
    "inspect:patterns": "bun run src/@inspection/supercharged-cli.ts --patterns",
    "inspect:export": "bun run src/@inspection/supercharged-cli.ts --format=markdown --output=inspection-report.md",
    "scope": "bun run src/@inspection/supercharged-cli.ts",
    "scope:interactive": "bun run src/@inspection/supercharged-cli.ts --interactive",
    "scope:stats": "bun run src/@inspection/supercharged-cli.ts --analytics --format=stats",
    "scope:html": "bun run src/@inspection/supercharged-cli.ts --format=html --output=scope.html",
    "scope:compare": "bun run scripts/compare-scopes.ts",
    "scope:security": "bun run src/@inspection/supercharged-cli.ts --security --patterns --analytics",
    "test": "bun test",
    "test:inspection": "bun test tests/enhanced-inspection-system.test.ts",
    "benchmark": "bun run src/@benchmarks/inspection-benchmarks.ts run",
    "benchmark:quick": "bun run src/@benchmarks/inspection-benchmarks.ts quick",
    "build": "bun build src/@inspection/supercharged-cli.ts --outdir=dist --target=bun",
    "dev": "bun --watch src/@inspection/supercharged-cli.ts"
  },
  "dependencies": {
    "jmespath": "^0.16.0",
    "jsonpath-plus": "^8.0.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "boxen": "^7.1.1",
    "ora": "^7.0.1",
    "inquirer": "^9.2.11",
    "commander": "^11.1.0",
    "yaml": "^2.3.4",
    "marked": "^9.1.6",
    "highlight.js": "^11.9.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.10.5",
    "@types/inquirer": "^9.0.7",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "duoplus",
    "inspection",
    "debugging",
    "cli",
    "jsonpath",
    "jq",
    "interactive",
    "analytics",
    "security",
    "patterns",
    "tui",
    "enterprise",
    "supercharged"
  ],
  "author": "DuoPlus Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/duoplus/duo-automation.git"
  },
  "bugs": {
    "url": "https://github.com/duoplus/duo-automation/issues"
  },
  "homepage": "https://duoplus.ai/packages/supercharged-inspection",
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./src/@inspection/index.ts",
      "bun": "./src/@inspection/supercharged-cli.ts",
      "default": "./src/@inspection/supercharged-cli.ts"
    },
    "./advanced": {
      "types": "./src/@inspection/commands/scope-advanced.ts",
      "default": "./src/@inspection/commands/scope-advanced.ts"
    },
    "./interactive": {
      "types": "./src/@inspection/commands/scope-interactive.ts",
      "default": "./src/@inspection/commands/scope-interactive.ts"
    },
    "./analytics": {
      "types": "./src/@inspection/commands/scope-analytics.ts",
      "default": "./src/@inspection/commands/scope-analytics.ts"
    },
    "./formatter": {
      "types": "./src/@inspection/commands/scope-formatter.ts",
      "default": "./src/@inspection/commands/scope-formatter.ts"
    }
  },
  "files": [
    "src/@inspection/**/*",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "duoplus-inspect": "src/@inspection/supercharged-cli.ts",
    "duoplus-scope": "src/@inspection/supercharged-cli.ts"
  },
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  }
}
