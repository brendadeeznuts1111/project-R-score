    "dashboard-health": "bun run scripts/unified-dashboard.js --health",
    "perf:dashboard": "bun run scripts/maintenance/perf-dashboard.ts",
    "perf:quick-fixes": "bun run scripts/maintenance/fix-sync-io.ts",
    "=== Bun-Native Dashboard ===": "",
    "dashboard:native": "bun run cli/dashboard.server.js",
    "dashboard:start": "DASHBOARD_SCOPE=ENTERPRISE bun run cli/dashboard.server.js",
    "dashboard:logs": "tail -f logs/dashboard-access.log",
    "dashboard:health": "bun scripts/health-monitor.ts",
    "dashboard:benchmark": "bun run cli/commands/dashboard-simple.ts dashboard benchmark",
    "build:dashboard": "bun run scripts/build-empire-cli.ts",
    "=== Pattern Matrix Commands ===": "",
    "pattern:register": "bun utils/pattern-cli.ts register",
    "pattern:list": "bun utils/pattern-cli.ts list",
    "pattern:phone-roi": "bun utils/pattern-cli.ts phone-roi",
    "pattern:report": "bun utils/pattern-cli.ts report",
    "pattern:generate-types": "bun -e \"import { patternMatrixLSP } from './src/utils/pattern-matrix-lsp'; const types = patternMatrixLSP.generatePatternTypes(); await Bun.write('./src/types/pattern-auto-generated.ts', types); console.log('âœ… LSP types generated'); console.log('ğŸ“Š Statistics:', JSON.stringify(patternMatrixLSP.getStatistics(), null, 2));\"",
    "pattern:lsp": "bun run pattern:generate-types",
    "=== Secrets Commands ===": "",
    "secrets:set": "bun utils/secrets-loader.ts --prompt",
    "secrets:update": "bun -e 'await (Bun as any).secrets.set({ service: \"windsurf-r2-empire\", name: process.argv[1], value: process.argv[2] })'",
    "secrets:delete": "bun -e 'await (Bun as any).secrets.delete({ service: \"windsurf-r2-empire\", name: process.argv[1] })'",
    "secrets:list": "bun -e \"import {secretManager} from './src/secrets/manager'; await secretManager.getRBACConfig(); console.log('Stored RBAC Users:', Object.keys((await secretManager.getRBACConfig()).users))\"",
    "secrets:migrate": "bun scripts/migrate-secrets.ts",
    "secrets:team1": "bun utils/secrets-loader.ts team1",
    "secrets:team2": "bun utils/secrets-loader.ts team2",
    "secrets:list-team": "bun -e \"import { scopedService } from './utils/secrets-loader'; import { secrets } from 'bun'; const team = process.argv[1] || 'team1'; const svc = await scopedService(team); const bucket = await secrets.get({ service: svc, name: 'R2_BUCKET' }); console.log(`Team: ${team} | R2: ${bucket || 'âš ï¸ None'}`);\" team1",
    "bench:secrets-win": "bun bench/bench-secrets-win.ts",
    "=== Analytics Commands ===": "",
    "analytics": "bun run cli/bin/windsurf-cli analytics",
    "cashapp-analytics": "bun run cli/bin/windsurf-cli analytics dashboard --cashapp",
    "analytics-report": "bun run cli/bin/windsurf-cli analytics report",
    "=== Query Commands ===": "",
    "query": "bun run scripts/query/query-r2-pattern.ts",
    "query-r2": "bun run scripts/query/query-r2-pattern.ts",
    "query-apple-ids": "bun run scripts/query/query-r2-pattern.ts --filter success=true country=US --prefix apple-ids/",
    "query-all": "bun run scripts/query/query-r2-pattern.ts --pattern -1",
    "query-export": "bun run scripts/query/query-r2-pattern.ts --export",
    "=== Pipeline Commands ===": "",
    "pipeline:e2e": "bun run scripts/e2e-simulation-pipeline.ts",
    "pipeline:bench": "bun run bench/bench-r2-real.ts",
    "pipeline:native": "bun run bench/bench-inline-native.ts",
    "pipeline:stream": "bun run docs/reference/MASTER_MATRIX.md",
    "=== Storage Commands ===": "",
    "storage": "bun run cli/bin/windsurf-cli storage",
    "storage-status": "bun run cli/bin/windsurf-cli storage status",
    "storage-upload": "bun run cli/bin/windsurf-cli storage upload --all",
    "storage-monitor": "bun run cli/bin/windsurf-cli storage monitor",
