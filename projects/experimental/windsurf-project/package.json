    "test-cashapp-cloud": "bun run scripts/cashapp/test-cashapp-cloud.js",
    "test-cashapp-cloud-mock": "bun run scripts/cashapp/test-cashapp-cloud.js --mock",
    "setup-real-sim": "bun run scripts/sim/setup-real-sim.js",
    "signup-cashapp-real": "bun run scripts/cashapp/signup-cashapp-real.js",
