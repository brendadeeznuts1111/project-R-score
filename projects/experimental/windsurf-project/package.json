    "deploy": "bun run cli/bin/windsurf-cli deploy",
    "deploy-workers": "bun run cli/bin/windsurf-cli deploy workers",
    "deploy-dns": "bun run cli/bin/windsurf-cli deploy dns",
    "deploy-cloudflare": "bun run cli/bin/windsurf-cli deploy cloudflare",
