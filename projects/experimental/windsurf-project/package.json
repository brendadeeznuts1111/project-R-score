    "track-sim-costs": "bun run scripts/sim/track-sim-costs.js",
    "verify-duoplus": "bun run scripts/maintenance/verify-duoplus-integration.js",
    "test-directory-organization": "bun run scripts/maintenance/test-directory-organization.js",
