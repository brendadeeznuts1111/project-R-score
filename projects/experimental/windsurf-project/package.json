{
  "name": "windsurf-project",
  "version": "2.4.0",
  "private": true,
  "type": "module",
  "bin": {
    "windsurf-cli": "./cli/bin/windsurf-cli",
    "windsurf-cli-enhanced": "./cli/bin/windsurf-cli-enhanced",
    "ep-cli": "./cli/bin/ep-cli"
  },
  "scripts": {
    "start": "bun run src/main.js",
    "init": "bun run src/main.js init",
    "test": "bun run src/main.js test",
    "test:enhanced": "bun test --preload ./tests/setup.ts --timeout 10000",
    "test:fast": "bun test --preload ./tests/setup.ts --timeout 5000 --bail",
    "test:concurrent": "bun test --preload ./tests/setup.ts --concurrent --timeout 5000",
    "test:cli": "bun test --preload ./tests/setup.ts tests/integration/dashboard/dashboard-integration.test.ts --timeout 5000",
    "=== Organized Test Categories ===": "",
    "test:unit": "bash scripts/test-runner.sh unit",
    "test:integration": "bash scripts/test-runner.sh integration",
    "test:performance": "bash scripts/test-runner.sh performance",
    "test:security": "bash scripts/test-runner.sh security",
    "test:e2e": "bash scripts/test-runner.sh e2e",
    "test:all": "bash scripts/test-runner.sh all",
    "test:list": "bash scripts/test-runner.sh list",
    "config:validate": "bun -e \"import config from './src/config/config-loader'; console.log(config.validate())\"",
    "config:generate": "bun -e \"import config from './src/config/config-loader'; config.generateEnvFile()\"",
    "=== CLI Commands ===": "",
    "windsurf": "bun cli/bin/windsurf-cli",
    "cli": "bun cli/bin/windsurf-cli",
    "cli:enhanced": "bun cli/bin/windsurf-cli-enhanced",
    "ep-cli": "bun cli/bin/ep-cli",
    "empire": "bun run cli/bin/ep-cli",
    "=== Creation Commands ===": "",
    "create": "bun run cli/bin/windsurf-cli create",
    "create-appleid": "bun run cli/bin/windsurf-cli create appleid",
    "create-cashapp": "bun run cli/bin/windsurf-cli create cashapp",
    "create-music": "bun run cli/bin/windsurf-cli create music",
    "create-batch": "bun run cli/bin/windsurf-cli create batch",
    "create-appleids": "bun run cli/bin/windsurf-cli create appleid --count=3",
    "create-cashapp-demo": "bun run cli/bin/windsurf-cli create cashapp --demo --count=3",
    "=== Documentation Commands ===": "",
    "changelog-parse": "bun run scripts/changelog-parser.ts",
    "changelog-stats": "bun run scripts/changelog-parser.ts --stats-only",
    "changelog-constants": "bun run scripts/changelog-parser.ts --constants-only",
    "docs-update": "bun run scripts/changelog-parser.ts && git add docs/changelogs/",
    "lint:md": "markdownlint '**/*.md' -i reports/ -i node_modules/ -i dist/ -i config/project/",
    "lint:md:fix": "markdownlint '**/*.md' -i reports/ -i node_modules/ -i dist/ -i config/project/ --fix",
    "lint:docs": "markdownlint 'docs/**/*.md' 'DEVELOPMENT_NOTES.md' -i reports/",
    "duoplus-parse": "bun run scripts/maintenance/parse-duoplus-log.ts",
    "duoplus-benchmark": "bun run utils/android-fingerprint.ts --full",
    "duoplus-simulate": "bun run utils/android-fingerprint.ts --simulate",
    "rpa-benchmark": "bun run src/core/rpa/tiktok-warmup.ts",
    "duoplus-integrate": "bun run scripts/maintenance/integrate-changelog.ts",
    "duoplus-full": "bun run scripts/maintenance/integrate-changelog.ts --benchmarks --update-dashboard",
    "master-matrix": "bun run scripts/maintenance/master-matrix.ts",
    "master:matrix": "bun scripts/maintenance/master-matrix.ts",
    "rpa:tiktok": "bun src/core/rpa/tiktok-warmup.ts",
    "rpa:reddit-fp": "bun src/core/rpa/reddit-fingerprint.ts",
