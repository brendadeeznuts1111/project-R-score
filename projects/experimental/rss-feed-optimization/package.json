{
  "name": "rss-feed-optimization",
  "version": "1.0.0",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "5.9.3"
  },
  "description": "High-performance RSS feed optimization system",
  "scripts": {
    "start": "bun run src/main.js",
    "start:prod": "bun run src/main.js --cluster",
    "start:debug": "bun run src/main.js --debug",
    "dev": "bun --hot src/server.js",
    "server": "bun run src/server.js",
    "test": "bun test",
    "test:runtime": "bun test tests/runtime.test.js",
    "benchmark": "bun run src/benchmark.js",
    "cli": "bun run cli.js",
    "init-rss": "bun run scripts/init-cli.js",
    "prepack": "node -e \"const pkg=require('./package.json');delete pkg.devDependencies;require('fs').writeFileSync('./package.json',JSON.stringify(pkg,null,2))\"",
    "postpack": "git checkout package.json"
  },
  "bin": {
    "init-rss": "./scripts/init-cli.js"
  },
  "dependencies": {},
  "keywords": [
    "rss",
    "feed",
    "optimization",
    "bun"
  ],
  "engines": {
    "bun": ">=1.3.8"
  },
  "author": "mike.hunt@factory.wager.com",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.factory-wager.com"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
