{
  "name": "t3-lattice-registry-client",
  "version": "1.2.1",
  "description": "Bun-native TypeScript implementation of T3-Lattice Registry Client",
  "main": "src/scripts/app.ts",
  "type": "module",
  "scripts": {
    "start": "bun run --env-file=.env.lattice src/scripts/app.ts",
    "lattice:start": "bun run --env-file=.env.lattice src/scripts/app.ts",
    "lattice:health": "bun run --env-file=.env.lattice src/scripts/health-check.ts",
    "lattice:metrics": "bun run --env-file=.env.lattice src/scripts/metrics-dashboard.ts",
    "lattice:audit": "bun run --env-file=.env.lattice src/scripts/audit-review.ts",
    "lattice:demo": "bun run --env-file=.env.lattice src/scripts/app.ts demo",
    "build": "bun build src/t3-lattice-registry.ts --outdir=dist",
    "test": "bun test",
    "lint": "bunx tsc --noEmit",
    "type-check": "bunx tsc --noEmit"
  },
  "keywords": [
    "t3-lattice",
    "registry",
    "typescript",
    "bun",
    "sports-analytics",
    "api-client"
  ],
  "author": "Sports Analytics Team",
  "license": "UNLICENSED",
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "mitata": "^1.0.34",
    "types": "^0.1.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/sports-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/sports-analytics/issues"
  },
  "homepage": "https://github.com/your-org/sports-analytics#readme"
}
