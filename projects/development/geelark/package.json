{
  "name": "geelark",
  "version": "1.0.0",
  "description": "Dev HQ - Advanced codebase analysis and automation platform built with Bun. Comprehensive developer toolkit with real-time monitoring, feature flags, and performance optimization.",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "dev-hq": "./dist/cli.js"
  },
  "author": "Dev HQ Team <dev-hq@example.com>",
  "license": "MIT",
  "keywords": [
    "bun",
    "typescript",
    "codebase-analysis",
    "automation",
    "developer-tools",
    "feature-flags",
    "performance-monitoring",
    "cli",
    "build-optimization",
    "dead-code-elimination",
    "real-time-dashboard",
    "web-socket",
    "http-server",
    "security-scanning",
    "bundle-analysis",
    "testing-framework",
    "runtime-features",
    "transpilation",
    "dependency-analysis",
    "git-insights"
  ],
  "engines": {
    "bun": ">=1.3.8",
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brendadeeznuts1111/geelark.git"
  },
  "bugs": {
    "url": "https://github.com/brendadeeznuts1111/geelark/issues"
  },
  "homepage": "https://github.com/brendadeeznuts1111/geelark#readme",
  "files": [
    "dist",
    "bin",
    "src",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.19.30",
    "commander": "^11.1.0",
    "chalk": "^5.6.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "dev": "bun --watch run src/index.ts",
    "dev:hot": "bun --hot run src/index.ts",
    "dev:debug": "bun --inspect --inspect-wait run src/index.ts",
    "dev:verbose": "bun --console-depth 5 run src/index.ts",
    "dev:smol": "bun --smol run src/index.ts",
    "dev:cli": "bun --hot --watch run bin/dev-hq-cli.ts insights --table",
    "build:dev": "bun build ./src/index.ts --outdir=./dist/dev --feature=ENV_DEVELOPMENT --feature=FEAT_EXTENDED_LOGGING --feature=FEAT_MOCK_API --minify --sourcemap",
    "build:prod-lite": "bun build ./src/index.ts --outdir=./dist/prod-lite --feature=ENV_PRODUCTION --feature=FEAT_ENCRYPTION --minify",
    "build:prod-standard": "bun build ./src/index.ts --outdir=./dist/prod-std --feature=ENV_PRODUCTION --feature=FEAT_AUTO_HEAL --feature=FEAT_NOTIFICATIONS --feature=FEAT_ENCRYPTION --feature=FEAT_BATCH_PROCESSING --minify",
    "build:prod-premium": "bun build ./src/index.ts --outdir=./dist/prod-premium --feature=ENV_PRODUCTION --feature=FEAT_PREMIUM --feature=FEAT_AUTO_HEAL --feature=FEAT_NOTIFICATIONS --feature=FEAT_ENCRYPTION --feature=FEAT_BATCH_PROCESSING --feature=FEAT_ADVANCED_MONITORING --feature=FEAT_VALIDATION_STRICT --minify",
    "build:android": "bun build ./src/index.ts --outdir=./dist/android --feature=ENV_PRODUCTION --feature=PLATFORM_ANDROID --feature=FEAT_ENCRYPTION --minify",
    "build:ios": "bun build ./src/index.ts --outdir=./dist/ios --feature=ENV_PRODUCTION --feature=PLATFORM_IOS --feature=FEAT_ENCRYPTION --minify",
    "build:web": "bun build ./src/index.ts --outdir=./dist/web --feature=ENV_PRODUCTION --feature=PLATFORM_WEB --feature=FEAT_ENCRYPTION --minify --target=browser",
    "build:test": "bun build --tsconfig-override .config/tsconfig.json --define ENV_DEVELOPMENT:true --define FEAT_MOCK_API:true src/index.ts --outdir dist/test",
    "build:audit": "bun run dev/scripts/build/build-validation.ts security-audit && bun build --tsconfig-override .config/tsconfig.json --define AUDIT_MODE:true --define FEAT_PREMIUM:true --define FEAT_AUTO_HEAL:true --define FEAT_NOTIFICATIONS:true --define FEAT_ENCRYPTION:true --define FEAT_BATCH_PROCESSING:true --define FEAT_ADVANCED_MONITORING:true --define FEAT_EXTENDED_LOGGING:true --define FEAT_VALIDATION_STRICT:true --no-macros --debug src/index.ts --outdir dist/audit",
    "build:compare": "bun run build:prod-lite && bun run build:prod-standard && bun run build:prod-premium && bun run dev/scripts/analysis/analyze-bundle.ts dist && ls -lh dist/prod-*",
    "build:analyze": "bun build --tsconfig-override .config/tsconfig.json --define ENV_PRODUCTION:true --define FEAT_PREMIUM:true --analyze src/index.ts",
    "build:dce": "bun run dev/scripts/build/build-validation.ts production-lite && bun build --tsconfig-override .config/tsconfig.json --define ENV_PRODUCTION:true --define FEAT_ENCRYPTION:true --minify --drop=console --jsx-side-effects src/index.ts --outdir dist/dce-optimized",
    "build:pure": "bun run dev/scripts/build/build-validation.ts production-standard && bun build --tsconfig-override .config/tsconfig.json --define ENV_PRODUCTION:true --define FEAT_PREMIUM:true --define FEAT_AUTO_HEAL:true --define FEAT_NOTIFICATIONS:true --define FEAT_ENCRYPTION:true --define FEAT_BATCH_PROCESSING:true --minify --drop=console --jsx-side-effects src/index.ts --outdir dist/pure-optimized",
    "build:security": "bun run dev/scripts/build/build-validation.ts security-audit && bun build --tsconfig-override .config/tsconfig.json --define AUDIT_MODE:true --define SECURITY_VALIDATION:true --no-macros --drop=console --jsx-side-effects src/index.ts --outdir dist/security",
    "build:report": "bun run --features=ENV_PRODUCTION ./src/index.ts --analyze > build-report.txt",
    "build:cli": "bun build bin/dev-hq-cli.ts --outfile dist/cli.js --minify --target bun",
    "dashboard:dev": "cd dashboard-react && bun run dev",
    "dashboard:build": "cd dashboard-react && bun run build",
    "dashboard:preview": "cd dashboard-react && bun run preview",
    "dashboard:serve": "bun run dev-hq/servers/dashboard-server.ts",
    "dashboard:serve:enhanced": "bun run dev-hq/servers/dashboard-server-enhanced.ts",
    "dashboard:serve:watch": "bun --watch run dev-hq/servers/dashboard-server.ts",
    "dashboard:serve:hot": "bun --hot run dev-hq/servers/dashboard-server.ts",
    "dashboard:serve:debug": "bun --inspect --inspect-wait run dev-hq/servers/dashboard-server.ts",
    "dev:dashboard": "bun run dashboard:build && bun run dashboard:serve",
    "build:all": "bun run dashboard:build && bun run build:prod-standard",
    "env:dev": "bun run dev/scripts/use-env.sh development",
    "env:staging": "bun run dev/scripts/use-env.sh staging",
    "env:prod": "bun run dev/scripts/use-env.sh production",
    "env:test": "bun run dev/scripts/use-env.sh test",
    "server:dev": "bun run dev/dev-hq/servers/dashboard-server.ts --env-file .env.development",
    "server:staging": "bun run dev/dev-hq/servers/dashboard-server.ts --env-file .env.staging",
    "server:prod": "bun run dev/dev-hq/servers/dashboard-server.ts --env-file .env.production",
    "server:test": "bun run dev/dev-hq/servers/dashboard-server.ts --env-file .env.test",
    "server:s3": "bun run dev/dev-hq/servers/dashboard-server.ts --upload-provider s3",
    "server:r2": "bun run dev/dev-hq/servers/dashboard-server.ts --upload-provider r2",
    "server:local": "bun run dev/dev-hq/servers/dashboard-server.ts --upload-provider local",
    "examples:file-io": "bun dev/examples/file-io-examples.ts",
    "examples:utils": "bun dev/examples/bun-utils-examples.ts",
    "examples:dce": "bun dev/examples/dce-examples.ts",
    "examples:all": "bun run examples:file-io && bun run examples:utils && bun run examples:dce",
    "build:safe": "bun build --ignore-dce-annotations src/index.ts",
    "build:safe:minify": "bun build --ignore-dce-annotations --minify src/index.ts",
    "build:dashboard:safe": "bun build --ignore-dce-annotations dashboard-react/src/index.tsx --outdir=./dist/dashboard-safe",
    "build:upload:safe": "bun build --ignore-dce-annotations --feature=FEAT_CLOUD_UPLOAD src/index.ts --outdir=./dist/upload-safe",
    "upload:lite": "bun run --feature=FEAT_CLOUD_UPLOAD ./src/index.ts",
    "upload:premium": "bun run --feature=FEAT_CLOUD_UPLOAD --feature=FEAT_UPLOAD_PROGRESS --feature=FEAT_MULTIPART_UPLOAD --feature=FEAT_UPLOAD_ANALYTICS --feature=FEAT_CUSTOM_METADATA ./src/index.ts",
    "build:upload-lite": "bun build ./src/index.ts --outdir=./dist/upload-lite --feature=FEAT_CLOUD_UPLOAD --minify",
    "build:upload-premium": "bun build ./src/index.ts --outdir=./dist/upload-premium --feature=FEAT_CLOUD_UPLOAD --feature=FEAT_UPLOAD_PROGRESS --feature=FEAT_MULTIPART_UPLOAD --feature=FEAT_UPLOAD_ANALYTICS --feature=FEAT_CUSTOM_METADATA --minify",
    "start:dev": "bun run --feature=ENV_DEVELOPMENT --feature=FEAT_MOCK_API ./src/index.ts",
    "start:prod": "bun run --feature=ENV_PRODUCTION ./src/index.ts",
    "start:premium": "bun run --feature=ENV_PRODUCTION --feature=FEAT_PREMIUM ./src/index.ts",
    "test": "bun test --config tests/config/bun-test.config.ts",
    "test:watch": "bun --watch test --config tests/config/bun-test.config.ts",
    "test:hot": "bun --hot test --config tests/config/bun-test.config.ts",
    "test:debug": "bun --inspect --inspect-brk test --config tests/config/bun-test.config.ts",
    "test:coverage": "bun test --config tests/config/bun-test.config.ts --coverage",
    "test:dev": "bun test --feature=ENV_DEVELOPMENT --feature=FEAT_MOCK_API",
    "test:prod": "bun test --feature=ENV_PRODUCTION",
    "test:premium": "bun test --feature=ENV_PRODUCTION --feature=FEAT_PREMIUM --feature=FEAT_ADVANCED_MONITORING",
    "test:unit": "bun test --config tests/config/bun-test.config.ts tests/unit/**/*.test.ts",
    "test:integration": "bun test --config tests/config/bun-test.config.ts tests/integration/**/*.test.ts",
    "test:e2e": "bun test --config tests/config/bun-test.config.ts tests/e2e/**/*.test.ts",
    "test:dev-hq": "bun test --config tests/config/bun-test.config.ts tests/integration/dev-hq/*.test.ts",
    "test:elimination": "bun test --config tests/config/bun-test.config.ts tests/unit/feature-elimination/*.test.ts",
    "test:types": "bun test --config tests/config/bun-test.config.ts tests/unit/type-testing/*.test.ts",
    "test:servers": "bun test --config tests/config/bun-test.config.ts tests/integration/server/*.test.ts tests/integration/api/*.test.ts",
    "test:ci": "bun test --config tests/config/bun-test.config.ts --coverage --reporter=json",
    "test:api": "bun test --config tests/config/bun-test.config.ts --user-agent \"geelark-api-tests/1.0\" tests/integration/api/",
    "test:server": "bun test --config tests/config/bun-test.config.ts --user-agent \"geelark-server-tests/1.0\" tests/integration/server/",
    "test:dev-hq-api": "bun test --config tests/config/bun-test.config.ts --user-agent \"geelark-dev-hq-tests/1.0\" tests/integration/dev-hq/",
    "test:geelark-api": "bun run dev/scripts/test-geelark-api.ts",
    "test:geelark-api:dry": "bun run dev/scripts/test-geelark-api.ts --dry-run",
    "test:mobile": "bun test --config tests/config/bun-test.config.ts --user-agent \"Mozilla/5.0 (iPhone; CPU iPhone OS 14_0 like Mac OS X) AppleWebKit/605.1.15\" tests/integration/api/",
    "test:desktop": "bun test --config tests/config/bun-test.config.ts --user-agent \"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36\" tests/integration/api/",
    "test:file-io": "bun test --config tests/config/bun-test.config.ts tests/unit/utils/bun-file-io.test.ts",
    "test:cli": "bun test --config tests/config/bun-test.config.ts tests/cli/**/*.test.ts tests/e2e/cli/**/*.test.ts",
    "test:cli:watch": "bun --watch test --config tests/config/bun-test.config.ts tests/cli/**/*.test.ts",
    "test:cli:debug": "bun --inspect --inspect-brk test --config tests/config/bun-test.config.ts tests/cli/**/*.test.ts",
    "test:dashboard": "bun test tests/unit/dashboard/",
    "test:dashboard:integration": "bun test tests/integration/dashboard/",
    "test:dashboard:all": "bun test tests/unit/dashboard/ && bun test tests/integration/dashboard/",
    "test:feature-flags": "bun test --config tests/config/bun-test.config.ts tests/unit/feature-flags/**/*.test.ts",
    "test:bun-runtime": "bun test --config tests/config/bun-test.config.ts tests/unit/bun-runtime/**/*.test.ts",
    "test:performance": "bun test tests/performance/**/*.test.ts",
    "test:networking": "bun test tests/performance/networking/**/*.test.ts",
    "test:transpilation": "bun test tests/performance/transpilation/**/*.test.ts",
    "test:configuration": "bun test tests/performance/configuration/**/*.test.ts",
    "test:benchmark": "bun test tests/performance/benchmark-utils.test.ts",
    "test:width": "bun run --feature=ENV_DEVELOPMENT ./src/index.ts --test-width",
    "bench": "bun run src/cli/benchmark-cli.ts",
    "bench:time": "bun run src/cli/benchmark-cli.ts time",
    "bench:memory": "bun run src/cli/benchmark-cli.ts memory",
    "bench:stress": "bun run src/cli/benchmark-cli.ts stress --iterations 500",
    "bench:compare": "bun run src/cli/benchmark-cli.ts compare",
    "bench:network": "bun run src/cli/benchmark-cli.ts network",
    "bench:transpilation": "bun run src/cli/benchmark-cli.ts transpilation",
    "bench:configuration": "bun run src/cli/benchmark-cli.ts configuration",
    "bench:profile": "bun --cpu-prof --cpu-prof-dir ./profiles run src/cli/benchmark-cli.ts time",
    "bench:runtime:expose-gc": "bun --expose-gc test bench/runtime-controls.bench.ts",
    "bench:process-lifecycle": "bun test bench/process-lifecycle.bench.ts",
    "bench:features": "bun test bench/bun-runtime-features.bench.ts",
    "benchmark": "bun run --feature=ENV_PRODUCTION ./src/benchmark.ts",
    "lint": "bun run dev/scripts/lint.ts",
    "lint:fix": "bun run dev/scripts/lint.ts --fix",
    "type-check": "bun run dev/scripts/type-check.ts",
    "clean": "rm -rf dist/*",
    "clean:all": "rm -rf dist/* node_modules/.cache",
    "analyze": "bun build --entrypoints=./src/index.ts --outdir=./dist/analyze --feature=ENV_PRODUCTION --minify --analyze",
    "analyze:bundle": "bun run dev/scripts/analysis/analyze-bundle.ts",
    "validate:features": "bun run --feature=ENV_PRODUCTION ./src/index.ts --validate-features",
    "generate:meta": "bun run dev/scripts/generate-meta.ts",
    "validate:meta": "bun run dev/scripts/validate-meta.ts",
    "update:checksum": "bun run dev/scripts/update-checksum.ts",
    "status": "bun run src/index.ts status",
    "logs": "bun run src/index.ts logs",
    "health": "bun run src/index.ts health",
    "audit": "bun run src/index.ts audit",
    "flags": "bun run src/index.ts flags",
    "dashboard": "bun run src/index.ts dashboard",
    "hq": "bun bin/dev-hq-cli.ts",
    "insights": "bun bin/dev-hq-cli.ts insights",
    "insights:table": "bun bin/dev-hq-cli.ts insights --table",
    "insights:json": "bun bin/dev-hq-cli.ts insights --json",
    "health:watch": "bun --watch run bin/dev-hq-cli.ts health",
    "version:current": "bun run --eval \"import { getCurrentVersion } from './src/utils/version'; console.log(getCurrentVersion())\"",
    "version:parse": "bun run --eval \"import { parseVersion, formatVersion } from './src/utils/version'; const v = process.argv[2]; console.log(JSON.stringify(parseVersion(v), null, 2))\" --",
    "version:next:major": "bun run --eval \"import { incrementVersion, getCurrentVersion } from './src/utils/version'; console.log(incrementVersion(getCurrentVersion(), 'major'))\"",
    "version:next:minor": "bun run --eval \"import { incrementVersion, getCurrentVersion } from './src/utils/version'; console.log(incrementVersion(getCurrentVersion(), 'minor'))\"",
    "version:next:patch": "bun run --eval \"import { incrementVersion, getCurrentVersion } from './src/utils/version'; console.log(incrementVersion(getCurrentVersion(), 'patch'))\"",
    "version:prerelease": "bun run --eval \"import { createPrerelease, getCurrentVersion } from './src/utils/version'; const pre = process.argv[2]; console.log(createPrerelease(getCurrentVersion(), pre))\" --",
    "version:compare": "bun run --eval \"import { compareVersions } from './src/utils/version'; const v1 = process.argv[2]; const v2 = process.argv[3]; const r = compareVersions(v1, v2); console.log(r < 0 ? '<' : r > 0 ? '>' : '=')\" --",
    "version:validate": "bun run --eval \"import { isValidVersion } from './src/utils/version'; const v = process.argv[2]; console.log(isValidVersion(v) ? 'Valid' : 'Invalid')\" --",
    "version:satisfies": "bun run --eval \"import { satisfiesRange } from './src/utils/version'; const v = process.argv[2]; const r = process.argv[3]; console.log(satisfiesRange(v, r) ? 'Yes' : 'No')\" --"
  },
  "publishConfig": {
    "registry": "https://registry.factory-wager.com"
  }
}
