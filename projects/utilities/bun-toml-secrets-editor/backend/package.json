{
	"name": "bun-payment-linker-backend",
	"version": "1.0.0",
	"description": "Enterprise-grade auto-underwriting platform with AI-powered risk assessment",
	"main": "src/index.js",
	"scripts": {
		"start": "node src/index.js",
		"start:optimized": "bun start-bun-optimized.js",
		"dev": "nodemon src/index.js",
		"dev:optimized": "bun --hot src/index.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"migrate": "knex migrate:latest",
		"migrate:rollback": "knex migrate:rollback",
		"seed": "knex seed:run",
		"build": "webpack --mode production",
		"docker:build": "docker build -t bun-payment-linker .",
		"docker:run": "docker run -p 3000:3000 bun-payment-linker",
		"lint": "eslint src/",
		"format": "prettier --write src/",
		"performance:demo": "bun start-bun-optimized.js",
		"benchmark:core": "bun benchmarks/bun-v1.3.7-benchmarks.js",
		"benchmark:backend": "bun benchmarks/backend-comparison.js",
		"benchmark:all": "bun benchmarks/run-benchmarks.js",
		"features:demo": "bun benchmarks/bun-v1.3.7-features-demo.js",
		"features:http": "bun benchmarks/bun-v1.3.7-http-features.js",
		"features:buffer": "bun benchmarks/buffer-optimization-demo.js",
		"features:complete": "bun benchmarks/bun-v1.3.7-complete-showcase.js",
		"features:refactor": "bun benchmarks/refactoring-demo.js",
		"features:cpu-profiling": "bun --cpu-prof --cpu-prof-md cpu-profiling-demo.js",
		"features:cpu-profiling-md": "bun --cpu-prof-md cpu-profiling-demo.js",
		"features:cpu-profiling-json": "bun --cpu-prof cpu-profiling-demo.js",
		"features:cpu-profiling-simple": "bun --cpu-prof --cpu-prof-md simple-cpu-profile.js",
		"features:cpu-profile-commit": "./auto-profile-commit.sh",
		"features:cpu-profile-commit-bypass": "./auto-profile-commit-bypass.sh",
		"features:heap-profiling": "bun --heap-prof --heap-prof-md heap-profiling-demo.js",
		"features:heap-profiling-md": "bun --heap-prof-md heap-profiling-demo.js",
		"features:heap-profiling-snapshot": "bun --heap-prof heap-profiling-demo.js",
		"features:heap-profile-commit": "./auto-heap-profile-commit.sh",
		"features:enhanced-heap-profiling": "bun --heap-prof-md enhanced-heap-profiling-demo.js",
		"features:enhanced-heap-profile-commit": "./enhanced-heap-profile-commit.sh",
		"features:optimized-heap-profiling": "bun --heap-prof-md optimized-heap-profiling-demo.js",
		"features:optimized-heap-profile-commit": "./optimized-heap-profile-commit.sh",
		"test:headers": "bun test-header-preservation.js",
		"profile:cpu": "bun --cpu-prof profile-backend.js",
		"profile:cpu-md": "bun --cpu-prof-md profile-backend.js",
		"profile:heap": "bun --heap-prof profile-backend.js",
		"profile:heap-md": "bun --heap-prof-md profile-backend.js",
		"profile:all": "bun --cpu-prof --cpu-prof-md --heap-prof --heap-prof-dir ./profiles profile-backend.js"
	},
	"dependencies": {
		"express": "^4.18.2",
		"cors": "^2.8.5",
		"helmet": "^7.1.0",
		"compression": "^1.7.4",
		"express-rate-limit": "^7.1.5",
		"express-validator": "^7.0.1",
		"dotenv": "^16.3.1",
		"stripe": "^14.9.0",
		"plaid": "^18.0.0",
		"axios": "^1.6.2",
		"ws": "^8.14.2",
		"socket.io": "^4.7.4",
		"redis": "^4.6.11",
		"pg": "^8.11.3",
		"knex": "^3.1.0",
		"bcryptjs": "^2.4.3",
		"jsonwebtoken": "^9.0.2",
		"joi": "^17.11.0",
		"winston": "^3.11.0",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"ethers": "^6.8.1",
		"web3": "^4.3.0",
		"node-cron": "^3.0.3",
		"bull": "^4.12.2",
		"ioredis": "^5.3.2",
		"csv-parser": "^3.0.0",
		"xlsx": "^0.18.5",
		"pdf-parse": "^1.1.1",
		"tesseract.js": "^5.0.4",
		"sharp": "^0.33.0",
		"node-forge": "^1.3.1",
		"uuid": "^9.0.1",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"chalk": "^4.1.2",
		"inquirer": "^8.2.6",
		"yaml": "^2.3.4"
	},
	"devDependencies": {
		"nodemon": "^3.0.2",
		"jest": "^29.7.0",
		"supertest": "^6.3.3",
		"eslint": "^8.55.0",
		"prettier": "^3.1.1",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"@babel/core": "^7.23.6",
		"@babel/preset-env": "^7.23.6",
		"babel-loader": "^9.1.3",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.29.0"
	},
	"keywords": [
		"fintech",
		"underwriting",
		"risk-assessment",
		"ai",
		"blockchain",
		"payments",
		"kyc",
		"compliance"
	],
	"author": "Bun Payment Linker Team",
	"license": "UNLICENSED",
	"engines": {
		"bun": ">=1.3.7",
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bun-payment-linker/backend"
	}
}
