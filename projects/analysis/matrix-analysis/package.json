  "dependencies": {
    "@modelcontextprotocol/server-filesystem": "latest",
    "@modelcontextprotocol/server-puppeteer": "latest",
    "@modelcontextprotocol/server-sequential-thinking": "latest",
    "@upstash/context7-mcp": "latest",
    "fuzzysort": "^3.1.0",
    "js-yaml": "^4.1.1",
    "lightningcss": "^1.31.1",
    "lru-cache": "^11.2.5",
    "mcp-bun": "latest",
    "rss-mcp": "^1.0.1",
    "semver": "^7.7.3",
    "ts-morph": "^21.0.1",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/bun": "latest",
    "@types/node": "latest",
    "@types/semver": "^7.7.1"
  },
  "mcp": {
    "servers": {
      "bun": {
        "command": "bun",
        "args": [
          "run",
          "node_modules/mcp-bun/dist/mcp-bun.js"
        ],
        "env": {
          "DISABLE_NOTIFICATIONS": "true"
        },
        "description": "Bun runtime integration for JavaScript/TypeScript execution (Bun native)"
      },
      "bun-docs": {
        "command": "bun",
        "args": [
          "mcp-bun-docs/index.ts"
        ],
        "description": "SearchBun - Search Bun docs (bun.com/docs/mcp). API refs, code examples, guides."
      },
      "context7": {
        "command": "bun",
        "args": [
          "run",
          "node_modules/@upstash/context7-mcp/dist/index.js"
        ],
        "description": "Context7 documentation and code search"
      },
      "filesystem": {
        "command": "bun",
        "args": [
          "run",
          "node_modules/@modelcontextprotocol/server-filesystem/dist/index.js",
          "/Users/nolarose"
        ],
        "description": "File system access for project operations"
      },
      "sequential-thinking": {
        "command": "bun",
        "args": [
          "run",
          "node_modules/@modelcontextprotocol/server-sequential-thinking/dist/index.js"
        ],
        "description": "Structured problem-solving and reasoning tool"
      },
      "puppeteer": {
        "command": "bun",
        "args": [
          "run",
          "node_modules/@modelcontextprotocol/server-puppeteer/dist/index.js"
        ],
        "description": "Browser automation for testing and web scraping",
        "env": {
          "HEADLESS": "true"
        }
      },
      "rss": {
        "command": "bun",
        "args": [
          "run",
          "node_modules/rss-mcp/dist/index.js"
        ],
        "description": "RSS feed reader and parser for news aggregation",
        "env": {
          "RSS_MAX_ITEMS": "50",
          "RSS_CACHE_TTL": "300"
        }
      }
    }
  },
  "keywords": [
    "bun",
    "mcp",
    "dashboard",
    "telemetry"
  ],
