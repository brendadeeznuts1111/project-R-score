{
  "name": "claudian",
  "version": "1.3.42",
  "description": "Claudian - Claude Code embedded in Obsidian sidebar",
  "main": "main.js",
  "scripts": {
    "build:css": "bun scripts/build-css.mjs",
    "dev": "bun run build:css && bun esbuild.config.mjs",
    "build": "bun scripts/build.mjs production",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "lint:fix": "bun run lint -- --fix",
    "test": "bun scripts/run-jest.js",
    "test:watch": "bun scripts/run-jest.js --watch",
    "test:coverage": "bun scripts/run-jest.js --coverage",
    "version": "bun scripts/sync-version.js && git add manifest.json"
  },
  "keywords": [
    "obsidian",
    "claude",
    "ai",
    "agent"
  ],
  "author": "Brenda Williams",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "20.19.30",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.27.2",
    "eslint": "8.57.1",
    "eslint-plugin-jest": "28.14.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "jest": "30.2.0",
    "obsidian": "latest",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "0.2.29",
    "tslib": "2.8.1"
  },
  "engines": {
    "bun": ">=1.3.8"
  },
  "publishConfig": {
    "registry": "https://registry.factory-wager.com"
  },
  "trustedDependencies": [
    "esbuild",
    "fsevents",
    "unrs-resolver"
  ]
}
