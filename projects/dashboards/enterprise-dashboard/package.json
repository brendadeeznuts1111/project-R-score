{
  "name": "enterprise-dashboard",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "bun": ">=1.3.8"
  },
  "scripts": {
    "prepare": "cp scripts/hooks/* .git/hooks/ 2>/dev/null && chmod +x .git/hooks/* 2>/dev/null && git config commit.template .gitmessage || true",
    "hooks:install": "cp scripts/hooks/* .git/hooks/ && chmod +x .git/hooks/* && git config commit.template .gitmessage && echo 'Hooks installed: pre-commit, commit-msg'",
    "dev": "bun run config:build:css && bun run css:build && bun --hot src/server/index.ts",
    "dev:debug": "bun run css:build && bun --hot src/server/index.ts --feature=DEBUG",
    "dev:css": "./node_modules/.bin/tailwindcss -i ./src/client/styles.css -o ./public/dist/styles.css --watch",
    "css:build": "./node_modules/.bin/tailwindcss -i ./src/client/styles.css -o ./public/dist/styles.css",
    "start": "NODE_ENV=production bun src/server/index.ts",
    "build": "bun build.ts",
    "build:free": "bun build.ts --variant=free",
    "build:premium": "bun build.ts --variant=premium",
    "build:debug": "bun build.ts --variant=debug",
    "build:lite": "bun build src/server/index.ts --target=bun --compile --minify --outfile=dist/dashboard-lite",
    "build:pro": "bun build src/server/index.ts --target=bun --compile --minify --outfile=dist/dashboard-pro",
    "build:standalone": "bun build src/server/index.ts --target=bun --compile --minify --outfile=dist/dashboard-standalone",
    "build:all": "bun run build:lite && bun run build:pro && bun run build:debug && echo 'Built: lite, pro, debug variants'",
    "topology": "bun scripts/topology.ts",
    "benchmark": "hyperfine './dist/dashboard-lite --help' './dist/dashboard-pro --help' --warmup 3",
    "bench:cold": "bun scripts/bench/cold-fetch.ts",
    "bench:preconnect": "bun scripts/bench/preconnect-fetch.ts",
    "bench:parallel": "bun scripts/bench/parallel-bench.ts",
    "bench:compare": "./scripts/bench/compare-preconnect.sh",
    "test": "bun test --preload ./test-setup.ts",
    "test:ci": "bun test --preload ./test-setup.ts --coverage --bail=1 --timeout 30000",
    "test:retry": "bun test --preload ./test-setup.ts --retry 3 --timeout 10000",
    "test:watch": "bun test --preload ./test-setup.ts --watch --timeout 15000",
    "test:grep": "bun test --preload ./test-setup.ts --grep",
    "test:debug": "bun test --inspect --timeout 0",
    "test:smol": "bun test --smol --timeout 5000",
    "test:verbose": "bun test --verbose --timeout 10000",
    "test:preload": "bun test --preload ./test-setup.ts",
    "test:kyc": "bun test src/server/kyc/__tests__",
    "test:kyc:watch": "bun test src/server/kyc/__tests__ --watch",
    "test:kyc:coverage": "bun test src/server/kyc/__tests__ --coverage",
    "test:kyc:performance": "bun test src/server/kyc/__tests__/performance.test.ts",
    "test:kyc:edge": "bun test src/server/kyc/__tests__/edgeCases.test.ts",
    "test:kyc:report": "bun run src/server/kyc/__tests__/generate-html-report.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "check": "bun test && tsc --noEmit",
    "ci": "bun test:ci && bun typecheck && bun config:lint",
    "ci:quick": "bun test --bail 1 && bun config:lint",
    "dev:all": "bun run css:build && (bun run dev:css & bun --hot src/server/index.ts)",
    "dev:open": "bun run dev & sleep 2 && open http://localhost:8080",
    "build:client": "bun build src/client/Dashboard.tsx --target=browser --outdir=public/dist --minify --splitting",
    "build:client:dev": "bun build src/client/Dashboard.tsx --target=browser --outdir=public/dist --react-fast-refresh",
    "build:ui": "bun run scripts/build-ui.ts",
    "build:ui:watch": "bun --hot run scripts/build-ui.ts",
    "performance:record": "bun run scripts/performance-monitor.ts record",
    "performance:report": "bun run scripts/performance-monitor.ts report",
    "secrets": "bun scripts/secrets.ts",
    "restart-server": "bun scripts/restart-server.ts",
    "colors:lint": "bun scripts/colors-lint.ts",
    "colors:lint:verbose": "bun scripts/colors-lint.ts --verbose",
    "colors:validate": "bun scripts/colors-lint.ts --no-warnings",
    "test:colors": "bun test src/client/__tests__/syntax-colors.test.ts",
    "bench:colors": "bun run src/client/__tests__/syntax-colors.bench.ts",
    "bench:crc32": "bun scripts/bench/crc32-bench.ts",
    "bench:spawn": "bun scripts/bench/spawn-bench.ts",
    "bench:archive": "bun scripts/bench/archive-bench.ts",
    "bench:v136": "bun scripts/bench/crc32-bench.ts && bun scripts/bench/spawn-bench.ts && bun scripts/bench/archive-bench.ts",
    "bench:mitata": "bun examples/bun-1.3.6-bench-mitata.ts",
    "bench:mitata:json": "BENCHMARK_RUNNER=1 bun examples/bun-1.3.6-bench-mitata.ts",
    "bench:crc32:mitata": "bun scripts/bench/crc32-bench-mitata.ts",
    "bench:spawn:mitata": "bun scripts/bench/spawn-bench-mitata.ts",
    "bench:all:mitata": "bun scripts/bench/crc32-bench-mitata.ts && bun scripts/bench/spawn-bench-mitata.ts && bun scripts/bench/archive-bench-mitata.ts && bun examples/bun-1.3.6-bench-mitata.ts",
    "bench:archive:mitata": "bun scripts/bench/archive-bench-mitata.ts",
    "bench": "cd scripts/bench && bun run all",
    "bench:json": "cd scripts/bench && bun run all:json",
    "validate:v136": "bun scripts/v1.3.6-validate.ts",
    "archive:syntax": "bun scripts/archive-syntax.ts",
    "startup:optimize": "bun scripts/startup-optimizations.ts",
    "startup:optimize:verbose": "bun scripts/startup-optimizations.ts --verbose",
    "startup:dry-run": "bun scripts/startup-optimizations.ts --dry-run",
    "config:lint": "bun scripts/lint/config-lint.ts",
    "config:lint:verbose": "bun scripts/lint/config-lint.ts --verbose",
    "config:validate": "bun scripts/lint/config-lint.ts --no-warnings",
    "config:validate:integrity": "bun run src/client/config/validate.ts",
    "config:edit": "bun run src/client/config/edit.ts",
    "config:edit:themes": "bun run config:edit themes",
    "config:edit:shortcuts": "bun run config:edit shortcuts",
    "config": "bun run scripts/config-cli.ts",
    "config:cli": "bun run scripts/config-cli.ts",
    "config:build:css": "bun run src/client/config/build-css.ts",
    "config:watch": "bun run --hot src/client/config/dev-server.ts",
    "config:status": "bun -e \"import c from './src/client/config/index.ts'; c.printConfigStatus()\"",
    "dev:config": "bun run --hot src/client/config/dev-server.ts",
    "build:themes": "bun run src/client/config/build-css.ts",
    "train:anomaly": "bun scripts/train-anomaly.ts",
    "train:anomaly:verbose": "bun scripts/train-anomaly.ts --verbose",
    "shortcuts:help": "bun scripts/shortcuts-help.ts",
    "shortcuts:list": "bun scripts/shortcuts-help.ts --shortcuts",
    "shortcuts:validate": "bun scripts/shortcuts-help.ts --validate",
    "shortcuts:export:json": "bun scripts/shortcuts-help.ts --format=json",
    "shortcuts:export:md": "bun scripts/shortcuts-help.ts --format=md",
    "shortcuts:export:html": "bun scripts/shortcuts-help.ts --format=html",
    "help:all": "bun scripts/shortcuts-help.ts --all",
    "analyze": "bun run cli/analyze.ts",
    "analyze:scan": "bun run cli/analyze.ts scan src/ --depth=3",
    "analyze:types": "bun run cli/analyze.ts types --exported-only",
    "analyze:classes": "bun run cli/analyze.ts classes --inheritance",
    "analyze:strength": "bun run cli/analyze.ts strength --by-complexity",
    "analyze:deps": "bun run cli/analyze.ts deps --circular",
    "diagnose": "bun run cli/diagnose.ts",
    "diagnose:health": "bun run cli/diagnose.ts health",
    "diagnose:painpoints": "bun run cli/diagnose.ts painpoints --top=10",
    "diagnose:grade": "bun run cli/diagnose.ts grade",
    "!": "bun run cli/bang.ts",
    "bang": "bun run cli/bang.ts",
    "bun:news": "bun run cli/bun-news.ts"
  },
  "dependencies": {
    "glob": "^13.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@happy-dom/global-registrator": "^20.4.0",
    "@tailwindcss/cli": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "latest",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "ajv": "^8.17.1",
    "happy-dom": "^20.4.0",
    "mitata": "^1.0.34",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "author": "mike.hunt@factory.wager.com",
  "license": "MIT",
  "description": "Enterprise dashboard with React, Tailwind, KYC compliance, and Bun-native build variants",
  "publishConfig": {
    "registry": "https://npm.factory-wager.com"
  },
  "trustedDependencies": [
    "bun",
    "fsevents"
  ]
}
