{
	"name": "@bun-toml/templates",
	"version": "1.0.0",
	"description": "Official bun-toml-secrets-editor templates and presets",
	"license": "MIT",
	"author": "bun-toml-secrets-editor",
	"repository": {
		"type": "git",
		"url": "https://github.com/bun-toml-secrets-editor/bun-toml-secrets-editor.git",
		"directory": "packages/templates"
	},
	"engines": {
		"bun": ">=1.3.7"
	},
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./src/index.ts",
			"types": "./src/index.ts"
		}
	},
	"scripts": {
		"build": "bun build ./src/index.ts --outdir ./dist --target bun --minify --splitting --sourcemap=external",
		"test": "bun test",
		"typecheck": "bunx tsc --noEmit",
		"clean": "rm -rf dist",
		"prepublishOnly": "bun run clean && bun run build && bun test"
	},
	"dependencies": {
		"@bun-toml/core": "workspace:*",
		"@bun-toml/shared": "workspace:*"
	},
	"devDependencies": {
		"bun-types": "latest",
		"typescript": "^5.9.3"
	},
	"files": [
		"dist/",
		"src/",
		"templates/",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"bun",
		"toml",
		"templates",
		"presets"
	]
}
