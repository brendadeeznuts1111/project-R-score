{
  "name": "factorywager-enterprise",
  "version": "5.1.0",
  "description": "ðŸš€ FactoryWager Enterprise Platform - Bun Native High-Performance Applications",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch server/server-enhanced.ts",
    "start": "bun run server/server-enhanced.ts",
    "start:content-type": "bun run server/content-type-server.ts",
    "mcp:security": "bun run lib/security/mcp-server.ts --http --port=3000",
    "build": "bun run scripts/build.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "benchmark": "bun run scripts/benchmark.ts",
    "lint": "bun run eslint lib/ --ext .ts",
    "lint:fix": "bun run eslint lib/ --ext .ts --fix",
    "format": "bun run prettier --write lib/**/*.ts",
    "format:check": "bun run prettier --check lib/**/*.ts",
    "type-check": "bun run tsc --noEmit",
    "clean": "rm -rf dist/ .cache/ coverage/",
    "setup": "bun install && bun run build",
    "security:audit": "bun audit",
    "security:check": "bun run scripts/security-check.ts",
    "metrics:generate": "bun run scripts/generate-metrics.ts",
    "docs:build": "bun run scripts/build-docs.ts",
    "docs:serve": "bun run scripts/serve-docs.ts",
    "release": "bun run scripts/release.ts",
    "deploy:staging": "bun run deployment/deploy-staging.sh",
    "deploy:production": "bun run deployment/deploy-production.sh",
    "test:unit": "bun test",
    "test:endpoints": "curl http://localhost:3000/test/fetch | bun -p",
    "test:content-types": "bun run tests/test-content-types.ts",
    "test:ui-quality": "bun run tests/test-ui-quality.ts",
    "test:accessibility": "bun run tests/test-accessibility.ts",
    "test:all": "bun run test:ui-quality && bun run test:accessibility && bun run test:content-types",
    "fetch:docs": "bun run examples/fetch-example.js",
    "fetch:advanced": "bun run services/advanced-fetch-service.ts",
    "content-type": "bun run services/content-type-demo.ts",
    "verbose": "bun run services/verbose-fetch-demo.ts",
    "urls": "curl http://localhost:3000/api/typedarray/urls | bun -p",
    "rss": "curl http://localhost:3000/feed/rss | bun -p",
    "demo": "curl http://localhost:3000/api/advanced-demo",
    "dashboard:ansi": "bun run lib/ansi-dashboard.ts",
    "dashboard:live": "bun -e \"import('./lib/ansi-dashboard.ts').then(m=>m.startLiveDashboard())\"",
    "docs:scrape": "bun run lib/docs-scraper.ts",
    "docs:validate": "bun -e \"import('./lib/docs-scraper.ts').then(m=>m.generateValidationReport().then(console.log))\"",
    "report:generate": "bun run lib/report-generator.ts",
    "markdown:ansi": "bun run lib/enhanced-markdown.ts README.md ansi",
    "url:validate": "bun run scripts/url-validator.ts check",
    "url:check": "bun run scripts/url-validator-focused.ts quick",
    "url:report": "bun run scripts/url-validator.ts check && cat url-validation-report.json",
    "url:optimize": "bun run scripts/optimize-examples-prefetch.ts optimize",
    "url:localhost-to-example": "bun run scripts/localhost-to-example.ts convert",
    "standards:check": "bun run url:check && bun run lint && bun run type-check",
    "ci:validate": "bun run standards:check && bun run test:all",
    "markdown:styled": "bun run lib/enhanced-markdown.ts docs/styling-demo.md ansi",
    "markdown:html": "bun run lib/enhanced-markdown.ts README.md html",
    "markdown:links": "bun run lib/enhanced-markdown.ts docs/api.md links",
    "markdown:headings": "bun run lib/enhanced-markdown.ts README.md headings",
    "markdown:plain": "bun run lib/enhanced-markdown.ts README.md plain",
    "markdown:official": "bun run lib/official-markdown-options.ts demo",
    "markdown:compare": "bun run lib/official-markdown-options.ts compare",
    "markdown:gfm": "bun run lib/official-markdown-options.ts gfm",
    "markdown:extended": "bun run lib/official-markdown-options.ts extended",
    "urls:extract": "bun -e \"const urls=[];Bun.markdown.render(await Bun.file(process.argv[2]||'docs/api.md').text(),{link:(t,{href})=>{urls.push(href);return null;},paragraph:()=>null,heading:()=>null});console.log(urls.join('\\n'));\"",
    "validate:pointers:save": "bun scripts/validate-pointers.ts --save",
    "validate:pointers:compare": "bun scripts/validate-pointers.ts --compare",
    "mcp:bun": "bun run lib/mcp/bun-mcp-server.ts",
    "mcp:tools": "bun run scripts/fw-tools-mcp.ts",
    "mcp:bridge": "bun run scripts/mcp-bridge.ts",
    "fw-docs": "bun run scripts/fw-docs.ts",
    "interactive-docs": "bun run scripts/interactive-docs.ts",
    "setup:mcp": "bash scripts/setup-mcp.sh",
    "test:r2": "bun run lib/mcp/r2-integration.ts",
    "demo:r2": "bun run scripts/demo-r2-mcp.ts",
    "r2": "bun run lib/r2/r2-enhanced-cli.ts",
    "r2:events": "bun run lib/r2/r2-enhanced-cli.ts events",
    "r2:batch": "bun run lib/r2/r2-enhanced-cli.ts batch",
    "r2:lifecycle": "bun run lib/r2/r2-enhanced-cli.ts lifecycle",
    "r2:search": "bun run lib/r2/r2-enhanced-cli.ts search",
    "r2:sync": "bun run lib/r2/r2-enhanced-cli.ts sync",
    "r2:backup": "bun run lib/r2/r2-enhanced-cli.ts backup",
    "r2:analytics": "bun run lib/r2/r2-enhanced-cli.ts analytics",
    "r2:security": "bun run lib/r2/r2-enhanced-cli.ts security",
    "r2:pipeline": "bun run lib/r2/r2-enhanced-cli.ts pipeline",
    "r2:webhook": "bun run lib/r2/r2-enhanced-cli.ts webhook",
    "r2:status": "bun run lib/r2/r2-enhanced-cli.ts status",
    "dashboard": "bun run dashboard/mcp-overview.ts status",
    "dashboard:monitor": "bun run dashboard/mcp-overview.ts monitor",
    "dashboard:web": "open dashboard/web-dashboard.html",
    "wiki:generate": "bun run lib/mcp/wiki-generator-mcp.ts generate",
    "wiki:templates": "bun run lib/mcp/wiki-generator-mcp.ts templates",
    "wiki:template": "bun run lib/mcp/wiki-generator-mcp.ts template",
    "wiki:history": "bun run lib/mcp/wiki-generator-mcp.ts history",
    "wiki:factorywager": "bun run lib/mcp/wiki-generator-mcp.ts factorywager",
    "wiki:matrix": "bun run scripts/wiki-matrix-cli.ts",
    "wiki:interactive": "bun run scripts/wiki-matrix-cli.ts interactive",
    "wiki:pipe": "bun run scripts/wiki-matrix-pipe.ts",
    "wiki:filter": "bun run scripts/wiki-matrix-filter.ts",
    "docs:search": "bun run cli/docs-cli.ts search",
    "docs:open": "bun run cli/docs-cli.ts open",
    "docs:index": "bun run cli/docs-cli.ts index",
    "docs:cache": "bun run cli/docs-cli.ts cache",
    "docs:install": "bash scripts/install-bun-docs.sh",
    "docs:demo": "bun run examples/bun-docs-demo.ts",
    "semver:demo": "bun run examples/bun-semver-demo.ts",
    "mybundocs11:create": "bun run cli/mybundocs11.ts create",
    "mybundocs11:launch": "bun run cli/mybundocs11.ts launch",
    "mybundocs11:open": "bun run cli/mybundocs11.ts open",
    "mybundocs11:info": "bun run cli/mybundocs11.ts info"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "jiti": "^2.6.1",
    "typescript-eslint": "^8.54.0"
  }
}