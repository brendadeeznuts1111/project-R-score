{
  "name": "systems-dashboard",
  "version": "1.0.0",
  "description": "üñ•Ô∏è Professional Systems Dashboard - Server monitoring with Bun inspect.table()",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "dev:port": "bunx --bun vite --port",
    "dev:host": "bunx --bun vite --host",
    "dev:https": "bunx --bun vite --https",
    "build": "bunx --bun vite build",
    "build:prod": "bun build --features=CLOUD_UPLOAD --minify src/main.ts --outdir dist/prod",
    "build:premium": "bun build --features=CLOUD_UPLOAD,PREMIUM,DEBUG --minify src/main.ts --outdir dist/premium",
    "build:dev": "bun build --features=LOCAL_DEV,DEBUG src/main.ts --outdir dist/dev",
    "build:all": "bun scripts/build-all.ts",
    "build:analyze": "bunx --bun vite build --mode analyze",
    "preview": "bunx --bun vite preview",
    "preview:port": "bunx --bun vite preview --port",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "bunx tsc --noEmit && bunx eslint . --ext .ts,.tsx",
    "lint:fix": "bunx eslint . --ext .ts,.tsx --fix",
    "format": "bunx prettier --write .",
    "format:check": "bunx prettier --check .",
    "s3:upload": "bun s3-upload.ts upload",
    "s3:validate": "bun s3-upload.ts validate",
    "r2:upload": "bun r2-upload.ts upload",
    "r2:validate": "bun r2-upload.ts validate",
    "upload:debug": "bun --features=DEBUG,CLOUD_UPLOAD src/upload-engine.ts",
    "upload:premium": "bun --features=CLOUD_UPLOAD,PREMIUM,METRICS src/upload-engine.ts",
    "ui:demo": "bun src/upload-progress-ui.ts",
    "router:demo": "bun src/url-pattern-router.ts",
    "router:examples": "bun src/url-pattern-examples.ts",
    "router:test": "bun src/url-pattern-examples.ts runAll",
    "router:official": "bun src/urlpattern-official-demo.ts",
    "azure:setup": "cp .env.azure .env",
    "azure:install": "bun install --config bunfig.toml",
    "azure:publish": "bun publish --config bunfig.toml",
    "clean": "rm -rf dist node_modules .bun",
    "start": "bun run preview",
    "serve": "bunx --bun vite preview --port 3000 --host",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage"
  },
  "bin": {
    "systems-dashboard": "./cli.js",
    "sysdash": "./cli.js"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "ora": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  }
}
